    "lint-staged": "^9.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "styled-reset": "^4.0.8",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
